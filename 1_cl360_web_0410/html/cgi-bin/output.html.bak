									success: function(ret) {
										if(ret == 1) {
											alert('$opt.success$');
											//refresh output page
											//setTimeout("jQuery('#menud').click();", 100);
					        				return true;	
											//closeAllPrompt();
										} else {
											return false;
										}
									}
