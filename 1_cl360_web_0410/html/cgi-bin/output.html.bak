	jQuery('#refreshBtn').click(function() {
		jQuery('#menud').click();
	});
	
	jQuery('#setBtn').click(function() {
		var validate = validateFormByID('wmform');
		if (validate) {
			var data = {"actioncode": "402", "rowCount": rowCount};
			var params = getFormItemValue('wmform');
			
			if(params) {
				jQuery.extend(data, params);
			}
			
			jQueryPostAjaxCmd({
				'data': data,
				success: function(ret) {
					if(ret == 1) {
						alert('$opt.success$');
						//刷新
						jQuery('#menud').click();
					}
				}
			});
		} else {
			return false;
		}
		
	});
	
	
	/*
	 * 添加弹出层初始化
	 */
	
	jQuery('#addSet').click(function() {
		//最多只能8个
		var length = jQuery('#streamTable tr').length;
		if(length >= 10) {
			return false;
		}
		
		closeAllPrompt();
		jQueryAjaxHtml({
			data: {"actioncode": "319"},
			success: function(innerStr) {
				art.dialog({
					title: '$output.add$',
					okValue: '$output.ok$',
					cancelValue: '$output.cancel$',
					lock: true,
					fixed: true,
				    background: '#600', // 背景色
				    opacity: 0.87,	// 透明度
				    content: innerStr,
				    ok: function () {
						var validate = validateFormByID('wmform_outputAdvancedSet');
						if(validate) {
							var data = {"actioncode": "403"};
							var params = getFormItemValue('wmform_outputAdvancedSet');
							
							var videoPort = params['videoPort'];
							var audioPort = params['audioPort'];
							
							if(videoPort == audioPort)
							{
								alert('$output.portSame$');
								return false;
							}
							
							if(params) {
								jQuery.extend(data, params);
								jQueryAjaxCmd({
									'data': data,
									success: function(ret) {
										if(ret == 1) {
											alert('$opt.success$');
											//refresh output page
											setTimeout("jQuery('#menud').click();", 100);
											//closeAllPrompt();
					        				return true;	
										} else if(ret == 3) {
											alert('$output.mustmultiIP$');
											return false;
										} else {
											return false;
										}
									}
								});
							}
						} else {
							return false;
						}
				    },
				    cancel: function() {
				    	closeAllPrompt();
				    	return true;
				    }
				});
				// 在chrome中有点不兼容
				var browser = navigator.userAgent.toLowerCase();
				if(browser.indexOf('chrome')!=-1 || browser.indexOf('safari')!=-1) {
					//setTimeout('formBeautify();', 300);
				} else {
					//formBeautify();
				}
				initFormValidation();
				
				
				jQuery('#tc_rate').prop('disabled', true);
				jQuery('#tc_flag').change(function() {
					var browser = navigator.userAgent.toLowerCase();
					if(jQuery(this).prop('checked')) {
						if (browser.indexOf('chrome') != -1 || browser.indexOf('safari') != -1) {
							jQuery('#tc_rate').prop('disabled', true);
						} else {
							jQuery('#tc_rate').prop('disabled', false);
						}
					} else {
						if (browser.indexOf('chrome') != -1 || browser.indexOf('safari') != -1) {
							jQuery('#tc_rate').prop('disabled', false);
						} else {
							jQuery('#tc_rate').prop('disabled', true);
						}
					}
				});
				
				//jQuery('#audioPort').prop('disabled', true);
				jQuery('#ip').prop('disabled', true);
				jQuery('#type').change(function() {

					if(jQuery('#type').val()=='1')
					{
						jQuery('#ip').prop('disabled', false);
						
						jQuery('#ip').val('234.255.0.90');
						jQuery('#videoPort').val('9500');
						jQuery('#audioPort').prop('disabled', true);
						jQuery('#audioPort').val('9500');
					}
					else if(jQuery('#type').val()=='2')
					{
						jQuery('#ip').prop('disabled', false);
					
						jQuery('#ip').val('240.10.6.80');
						jQuery('#videoPort').val('8800');
						jQuery('#audioPort').prop('disabled', false);
						jQuery('#audioPort').val('8802');
					}
					else	if(jQuery('#type').val()=='3')
					{
						
						jQuery('#ip').prop('disabled', true);
						jQuery('#ip').val('127.0.0.1');
						jQuery('#videoPort').val('6400');
						jQuery('#audioPort').prop('disabled', false);
						jQuery('#audioPort').val('6402');
					}
					else if(jQuery('#type').val()=='4')
					{
						jQuery('#ip').prop('disabled', false);
						jQuery('#ip').val('239.5.6.7');
						jQuery('#videoPort').val('7700');
						jQuery('#audioPort').prop('disabled', true);
						jQuery('#audioPort').val('7700');
					}

		/*			if(jQuery('#type').val()=='1' || jQuery('#type').val()=='4') {
						jQuery('#audioPort').prop('disabled', true);
					} else {
						jQuery('#audioPort').prop('disabled', false);
					}
					
					if(jQuery('#type').val()=='3') {
						jQuery('#ip').prop('disabled', true);
						jQuery('#ip').val('127.0.0.1');
					}
					else{
						jQuery('#ip').prop('disabled', false);
						jQuery('#ip').val('234.255.0.90');
					}
					*/
