<script>
	
/*
 * 初始化表格行数据
 */

var rowCount = 0;
function initTableData(dataArray) {
	if(dataArray && typeof dataArray == 'object') {
		rowCount = dataArray.length;
		for(var i=0; i<dataArray.length; i++) {
			var item = dataArray[i];
			if(item) {
				//增加行
//				for (var i = 0; i < 2; i++) {
//				var item = {'rowIndex': '1', 'type': '2', 'ip': '192.168.0.3', 'videoPort':'1260', 'status': '3'};
					var index = i + 1;
					var inputStream = item['inputStream'];
					var playurl = item['playurl'];
					var rowIndex = item['rowIndex'];
					var protocal = '';
					var type = item['type'];
					var ip = item['ip'];
					var videoPort = item['videoPort'];
					var audioPort = item['audioPort'];
					var status = item['status'];
					var permissions = item['permissions'];
					var permis = "";
					
					if(permissions){
						permis = "$outputdetailset.permissionsEnable$";
					}else{
						permis = "$outputdetailset.permissionsDisable$";
					}

					var playurTd = '<td style="width: 250px;">' + playurl + '</td>';

					switch(type) {
						case '0': {
							protocal = 'INVALID';
							break;
						}
						case '1': {
							protocal = 'TS';
							break;
						}
						case '2': {
							protocal = 'RTP';
							playurTd = '<td><a href="encoder.cgi?actioncode=420&ip='+ ip +'&vPort='+videoPort+'&aPort='+audioPort+'&iStream=' + inputStream +'">SDP Download</a></td>';
							break;
						}
						case '3': {
							protocal = 'RTSP';
							break;
						}
						case '4': {
							protocal = 'TS_OVER_RTP';
							break;
						}
					}

					var trInnerStr = '<tr><td height="35" class="$image_prex$itemnametd"><h6 name="index' + index + '" align="center">' + index + '</h6></td>';
					//trInnerStr += '<td><input name="inputStream' + index + '" readonly class="show_label" value="' + inputStream + '" /></td>';
					trInnerStr += '<td><input name="protocal' + index + '" readonly class="show_label" value="' + protocal + '" /><input type="hidden" name="rowIndex' + index + '" value="' + rowIndex + '"/><input type="hidden" name="type' + index + '" value="' + type + '"/></td>';
					trInnerStr += playurTd;
					trInnerStr += '<td style="display:none"><input name="ip' + index + '" readonly class="show_label" value="' + ip + '" /></td>';
					trInnerStr += '<td style="display:none"><input name="videoPort' + index + '" readonly class="show_label" value="' + videoPort + '" /></td>';
					trInnerStr += '<td><input name="permissions' + index + '" readonly class="show_label" value="' + permis + '" /></td>';
					trInnerStr += '<td><select style="width:100px;" name="status' + index + '">';
					
					switch(status) {
						case '1':{
							trInnerStr += '<option selected value="1">$output.active$</option><option value="3">$output.stop$</option><option value="4">$output.remove$</option>';
							break;
						}
						case '3':{
							trInnerStr += '<option value="1">$output.active$</option><option selected value="3">$output.stop$</option><option value="4">$output.remove$</option>';
							break;
						}
						case '4':{
							trInnerStr += '<option value="1">$output.active$</option><option value="3">$output.stop$</option><option selected value="4">$output.remove$</option>';
							break;
						}
					}
					trInnerStr += '</select><a class="detailSet" style="margin-left:10px;" href="#">$output.detail$</a></td></tr>';
					jQuery(trInnerStr).insertBefore('#addSetTr');
//				}
			}
		}
		
		 //添加记录成功后，隐藏添加按钮
		 if(rowCount > 0)
		 {
		 	document.getElementById("addSet").style.visibility = "hidden";
		 }else{
		 	document.getElementById("addSet").style.visibility = "visible";
		 }
	}
}


/*
 * 修改弹出层初始化
